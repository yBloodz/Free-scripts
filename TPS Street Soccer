local Player = game.Players.LocalPlayer
local RootPart = Player.Character.HumanoidRootPart

local Players = game:GetService("Players")
local player = Player

-- Locate the ScreenGui in the Player's PlayerGui
local ScreenGuiDiscord = game.CoreGui:FindFirstChild("Discord")

if not ScreenGuiDiscord then
    ScreenGuiDiscord = nil
end

local Neverlose_Main = loadstring(game:HttpGet("https://raw.githubusercontent.com/Mana42138/Neverlose-UI/main/Source.lua"))()
local Win = Neverlose_Main:Window({
    Title = "TPS Neverlose",
    CFG = "TPS Street Soccer",
    Key = Enum.KeyCode.H,
    External = {
        KeySystem = false,
        Key = {
            "2025",
            "yBloodz"
        }
    }
})

local TabSection1 = Win:TSection("Main")

local Autofarm = TabSection1:Tab("All GUI's") 

local AutoFSec1 = Autofarm:Section("Main")

local TestFarm1 = AutoFSec1:Toggle("AntiCheat Bypasser", function(arg)
      	if arg then
	loadstring(game:HttpGet("https://freenote.biz/raw/vyzuuBLBkX"))()
	end
end)

local sky = AutoFSec1:Toggle("Purple Sky", function(arg)
    if arg then
      		local skybox = Instance.new("Sky")
skybox.Parent = game.Lighting
skybox.SkyboxBk = "http://www.roblox.com/asset/?id=159454299"
skybox.SkyboxDn = "http://www.roblox.com/asset/?id=159454296"
skybox.SkyboxFt = "http://www.roblox.com/asset/?id=159454293"
skybox.SkyboxLf = "http://www.roblox.com/asset/?id=159454286"
skybox.SkyboxRt = "http://www.roblox.com/asset/?id=159454300"
skybox.SkyboxUp = "http://www.roblox.com/asset/?id=159454288"
skybox.StarCount = 5000
  	end    
end)

local NightSky = AutoFSec1:Toggle("Auto Farm", function(t)
    if arg then
	    getgenv().farmer = true
	    while wait() do
	    if getgenv().farmer == true then
	    game.Lighting.TimeOfDay = "00:00:00"
	    end
	    end
    end
end)

local function destroyUi()
    if ScreenGuiDiscord then
        destroyUi()
    else
        ScreenGuiDiscord = nil
    end
end

